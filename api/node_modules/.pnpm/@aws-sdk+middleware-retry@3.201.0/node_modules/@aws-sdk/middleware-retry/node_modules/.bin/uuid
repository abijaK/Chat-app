#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/bin-caleb/Documents/GDAProjects/BACKEND/Projet du mois/Chat-app/api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/bin-caleb/Documents/GDAProjects/BACKEND/Projet du mois/Chat-app/api/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../uuid@8.3.2/node_modules/uuid/dist/bin/uuid" "$@"
else
  exec node  "$basedir/../../../../../../uuid@8.3.2/node_modules/uuid/dist/bin/uuid" "$@"
fi
